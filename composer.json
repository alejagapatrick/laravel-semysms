{
    "name": "allanvb/laravel-semysms",
    "description": "SemySMS integration for Laravel",
    "keywords": ["laravel", "semysms"],
    "homepage": "https://github.com/allanvb/laravel-semysms",
    "license": "MIT",
    "authors": [
        {
            "name": "Alan Vb",
            "email": "alan.vb@mail.ru"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "^6.3"
